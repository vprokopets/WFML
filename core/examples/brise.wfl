abstract Region {
	sa: SamplingStrategy
}

abstract SamplingStrategy {
    RegionID -> string
    test -> integer
    xor Strategy {
        Sobol
        MersenneTwister
    }
    [test > 2]
   [RegionID in Context.ExperimentRegions.Regionss]
}

Context {
    ExperimentRegions {
        Regionss -> array
		[fcard.Regions = size Regionss]
		Regions: Region
    }
}
