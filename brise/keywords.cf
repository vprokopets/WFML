Test {
    Regions -> predefined
    [Regions = unique Region in Feature1]
}

abstract Feature {
    Region -> predefined
    test -> predefined
}


Feature1: Feature {
    [Region = 4]
    [test = childs.self]
    Feature11: Feature {
        [Region = 1]
        [test = parent.self.Region]
        Feature111: Feature + {
            [Region = 2]
            [test = path.self]
        }
    }
}

