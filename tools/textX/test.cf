abstract process{
    xor mode {
        manual{
            parameter1 -> integer
        }
        automatic{
            parameter2 -> integer
        }
    }
}
process1 : process{
    [mode.manual.parameter1 = 2]
    a -> integer
    [if mode.manual.parameter1 > 5 then a = 100 else a = 100500]
}

process2 : process{
    [mode.manual.parameter1 = 10]
    a -> integer
    b -> integer
    [if process1.mode.manual.parameter1 > 5 then a = 100 else a = 100500]
    [if mode.manual.parameter1 > 5 then b = 100 else b = 100500]
}